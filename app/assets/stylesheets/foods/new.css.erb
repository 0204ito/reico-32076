.food-page{
  display: flex;
  background-image: url("<%= asset_path('cook1.png') %>");
  background-size: cover;
  width: 100%;

}
.new-food{
  margin: 10px auto;

}
.new-food-title{
  margin: 10px 0 10px 30px;
  color: white;
  font-size: 20px;
}
.new-foodsub-title{
  color: white;
  font-size: 15px;
  margin-bottom: 10px;
  margin-left: 30px;
}
.food-spece{
width: calc(100vw - 220px);
background-color: white;
border-radius: 15px;
padding: 25px;
}

